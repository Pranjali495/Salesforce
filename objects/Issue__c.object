    <validationRules>
        <fullName>Cannot_fix_without_resolution_note</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Status__c , &quot;Fixed&quot;) &amp;&amp; ISBLANK( Resolution_Note__c ) &amp;&amp;   NOT(ISPICKVAL(PRIORVALUE(Status__c ), &quot;Fixed&quot;) )</errorConditionFormula>
        <errorDisplayField>Resolution_Note__c</errorDisplayField>
        <errorMessage>Cannot set Issue to &quot;Fixed&quot; without providing Resolution Note</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_issue_cant_have_assignee</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Status__c , &quot;New&quot;)  &amp;&amp;  NOT( ISBLANK( Assigned_To__c ) )</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>A &quot;New&quot; issue cannot have an assignee.  Set it to &quot;Assigned&quot; instead.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Assign_To</fullName>
        <active>true</active>
        <description>If status is anything other than &apos;new&apos; assign to is required.</description>
        <errorConditionFormula>AND( ISBLANK(Assigned_To__c), NOT(ISPICKVAL(Status__c, &quot;New&quot;)))</errorConditionFormula>
        <errorMessage>This Issue must be assigned to someone.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>