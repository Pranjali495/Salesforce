        <fullName>Submit_for_Approval</fullName>
        <availability>online</availability>
        <description>Validates the opportunity meets entry criteria for the approval process, presents custom message, and prevents submission if criteria are not met.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit for Approval</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)}
//{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/apex.js&quot;)}

//var currencyRecord=sforce.apex.execute(&quot;CurrencyConversion&quot;,&quot;isExistingOpportunity&quot;,{valueId:&apos;{! Opportunity.Id}&apos;});
var currencyRecord = sforce.connection.query(&quot;select Id from Opportunity where Total_Addressable_Revenue__c &gt; USD2000000 and Id = &apos;{!Opportunity.Id}&apos;&quot;);

if (&apos;{!Opportunity.Amount}&apos; == &apos;&apos;) { 
   alert(&apos;Opportunity Amount is required before submitting for approval.&apos;); 
} 
else if(currencyRecord.size == 0) { 
   alert(&apos;Total Addressable Revenue must be greater than USD 2,000,000 before submitting for approval.&apos;); 
} 
else if (&apos;{!Opportunity.Approval_Status__c}&apos; == &apos;Approved&apos;) {
   alert(&apos;Opportunity and/or products must be modified before re-submitting for approval.&apos;); 
}
else if(&apos;{!Opportunity.Approval_Status__c}&apos; == &apos;Pending Approval&apos;) {
   alert(&apos;This record is currently in an approval process. A record can be in only one approval process at a time.&apos;);
}
else if (&apos;{! Opportunity.StageName }&apos; == &apos;(SaaS) Lead - 10%&apos; || 
         &apos;{! Opportunity.StageName }&apos; == &apos;(SaaS) Discovery Phase - 20%&apos; || 
         &apos;{! Opportunity.StageName }&apos; == &apos;(SaaS) Meeting Confirmed - 30%&apos; || 
         &apos;{! Opportunity.StageName }&apos; == &apos;(SaaS) Opportunity Identified - 35%&apos; || 
         &apos;{! Opportunity.StageName }&apos; == &apos;(SaaS) Actively Engaged - 40%&apos; || 
         &apos;{! Opportunity.StageName }&apos; == &apos;(SaaS) Opportunity Lost - 0%&apos; ||
         &apos;{! Opportunity.StageName }&apos; == &apos;(Ads) Lead - 10%&apos; || 
         &apos;{! Opportunity.StageName }&apos; == &apos;(Ads) Discovery Phase - 20%&apos; ||
         &apos;{! Opportunity.StageName }&apos; == &apos;(Ads) Meeting Confirmed - 30%&apos; || 
         &apos;{! Opportunity.StageName }&apos; == &apos;(Ads) Opportunity Identified - 35%&apos; ||
         &apos;{! Opportunity.StageName }&apos; == &apos;(Ads) RFP Received - 40%&apos; || 
         &apos;{! Opportunity.StageName }&apos; == &apos;(Ads) Closed/Lost - 0%&apos; ||
         &apos;{! Opportunity.StageName }&apos; == &apos;(SaaS) Change in Performance - (35%)&apos; ||
         &apos;{! Opportunity.StageName }&apos; == &apos;(SaaS) Client Termination - (35%)&apos; ||
         &apos;{! Opportunity.StageName }&apos; == &apos;(SaaS) Product Termination - (35%)&apos; ||
         &apos;{! Opportunity.StageName }&apos; == &apos;(SaaS) Product Termination - (75%)&apos; ||
         &apos;{! Opportunity.StageName }&apos; == &apos;(SaaS) Change in Performance - (100%)&apos; ||
         &apos;{! Opportunity.StageName }&apos; == &apos;(SaaS) Client Termination - (100%)&apos;) {
   alert(&apos;Opportunity Probability must be greater than 50% before submitting for approval.&apos;);
}
else {
   if(confirm(&apos;Once you submit this record for approval, you might not be able to edit it or recall it from the approval process depending on your settings. Continue?&apos;)) {
      window.location.href=&apos;{!URLFOR($Action.Opportunity.Submit, Opportunity.Id )}&apos;;
   }
}</url>
    </webLinks>
