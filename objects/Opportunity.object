        <errorConditionFormula>and( 
(OR( $User.ProfileId =&quot;00eF0000000awFT&quot;,$User.ProfileId =&quot;00eF0000000awFU&quot;,$User.ProfileId =&quot;00eF0000000awFO&quot;,$User.ProfileId =&quot;00eF0000001657I&quot;,$User.ProfileId =&quot;00eF0000000i6aO&quot;, $User.ProfileId =&quot;00eF0000001CBwd&quot;, $User.ProfileId =&quot;00eF0000000awFS&quot;, $User.ProfileId =&quot;00eF0000000i6sI&quot;, $User.ProfileId = &quot;00e0G000001EOE3&quot;, $User.ProfileId = &quot;00eF0000001IU07&quot;)), 
NOT(ISPICKVAL(Type,&quot;Change in Performance&quot;)), NOT(ISPICKVAL(Type,&quot;Termination&quot;)), 
Probability &gt;= 0.50, 
IF( 
or(ISBLANK(Web_Online_Perc_Addressable__c) ,ISBLANK(Web_Online_Revenue__c),ISBLANK( Phone_Contact_Center_Revenue__c ), ISBLANK(Phone_Contact_Center_Addressable__c), ISBLANK(Mobile_Revenue__c ), ISBLANK(Mobile_Addressable__c), ISBLANK(In_Store_Revenue__c), ISBLANK(In_Store_Addressable__c)), 
