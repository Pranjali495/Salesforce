    <validationRules>
        <fullName>Hours_Required</fullName>
        <active>true</active>
        <description>This rule requires the hours field for new records when Integration field is checked.</description>
        <errorConditionFormula>AND(
ISPICKVAL(Family,&apos;Integration&apos;),
ISNULL(Hours__c)
)</errorConditionFormula>
        <errorDisplayField>Hours__c</errorDisplayField>
        <errorMessage>Please enter the number of hours.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Product_Category_Sub_Category</fullName>
        <active>true</active>
        <description>This rule requires the Product Category (ProductFamily) and Product Sub-Category fields for product records.</description>
        <errorConditionFormula>OR(
ISBLANK(TEXT(Family)),
ISBLANK(TEXT(Product_Sub_Category__c))
)</errorConditionFormula>
        <errorMessage>Product Category and Product Sub-Category are required.</errorMessage>
    </validationRules>
