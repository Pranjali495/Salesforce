    <validationRules>
        <fullName>Only_sysadmin_can_change_owner</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Id  &lt;&gt; &quot;00e0G000000i87w&quot;,$Profile.Id  &lt;&gt; &quot;00e0G0000014wpx&quot;,$Profile.Id  &lt;&gt; &quot;00eF0000000awFL&quot;,$Profile.Id  &lt;&gt; &quot;00eF00000016579&quot;,AND($Profile.Id  &lt;&gt; &quot;00eF0000000awFP&quot;, RecordType.Id &lt;&gt; &quot;012F0000000qjB0&quot;    )) &amp;&amp; ISCHANGED( OwnerId )</errorConditionFormula>
        <errorDisplayField>OwnerId</errorDisplayField>
        <errorMessage>Only system admins can change the ownership.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Support_Package_for_Customers</fullName>
        <active>true</active>
        <description>Support Package is required when the Account Relationship changes to &quot;Customer&quot;.</description>
        <errorConditionFormula>and
(
ISPICKVAL( Account_Relationship__c , &quot;Customer&quot;), 
ISBLANK( text(Support_Package__c )) 
)</errorConditionFormula>
        <errorDisplayField>Support_Package__c</errorDisplayField>
        <errorMessage>For Customers, you must select a Support Package before the record can be saved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Support_Region</fullName>
        <active>false</active>
        <description>Support Region only required when Relationship = Customer.</description>
        <errorConditionFormula>and
(
ISPICKVAL( Account_Relationship__c , &quot;Customer&quot;), 
ISBLANK( text(Support_Region__c )) 
)</errorConditionFormula>
        <errorDisplayField>Support_Region__c</errorDisplayField>
        <errorMessage>Support Region only required when Relationship = Customer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Support_Region_for_Partners</fullName>
        <active>true</active>
        <description>Support Region is required for Accounts that are partners</description>
        <errorConditionFormula>and 
( 
ISPICKVAL( Type , &quot;Partner&quot;), 
ISBLANK( text(Support_Region__c )),
NOT($Profile.Name = &quot;System Administrator&quot;)
)</errorConditionFormula>
        <errorDisplayField>Support_Region__c</errorDisplayField>
        <errorMessage>A Support Region is required for all partners so that cases opened by partners can be routed to the correct support team.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>User_cannot_change_Account_Name</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Id &lt;&gt;&quot;00e0G000000i87w&quot;,$Profile.Id &lt;&gt; &quot;00eF0000000awFL&quot;,$Profile.Id &lt;&gt; &quot;00eF00000016579&quot; ,$UserRole.Id &lt;&gt; &quot;00EF00000015a10&quot;, $UserRole.Id &lt;&gt; &quot;00EF00000015a2I&quot; , OR($Profile.Id &lt;&gt; &quot;00eF0000000awFP&quot;, RecordType.Id == &quot;012F0000000qjB0&quot; )) &amp;&amp; ISCHANGED(  Name )</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Only system admins can change Account Names</errorMessage>
    </validationRules>
