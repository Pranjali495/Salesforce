    <validationRules>
        <fullName>Contract_Type_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISBLANK(TEXT(ContractType__c)),
OR(
ISPICKVAL(Product2.Family,&apos;Services&apos;),
ISPICKVAL(Product2.Family,&apos;Integration&apos;)
),
NOT($Profile.Name = &quot;System Administrator&quot;),
NOT($Profile.Name = &quot;Super User&quot;),
Quote.Opportunity.Probability &gt;= 0.5
)</errorConditionFormula>
        <errorDisplayField>ContractType__c</errorDisplayField>
        <errorMessage>Please select a contract type for the service and integration products. This is required.</errorMessage>
    </validationRules>
