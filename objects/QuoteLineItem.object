        <externalId>false</externalId>
        <formula>IF(ISPICKVAL ( Interval__c  , &quot;Monthly&quot;),  UnitPrice/12 , 
IF(ISPICKVAL(Interval__c , &quot;Quarterly&quot;), UnitPrice/4,UnitPrice))</formula>
