        <fullName>Unqualified_Reason_Required</fullName>
        <active>true</active>
        <description>If Status is Manually Unqualified and Unqualified reason is empty a message will prompt the user to select an option</description>
        <errorConditionFormula>AND( 
ISPICKVAL( Status , &quot;Manually Unqualified&quot;), 
ISPICKVAL( Unqualified_Reason__c , &quot;&quot; ) 
)</errorConditionFormula>
        <errorDisplayField>Unqualified_Reason__c</errorDisplayField>
        <errorMessage>Please select an Option for Unqualified Reason</errorMessage>
    </validationRules>
